<style type="text/css">
	.ent-content{
		background-color: #f5f5f5;
	}
	.ent-serach-wrap{
		height: 40px;
		width: 300px;
		margin: 0 auto;
		border: 1px solid rgb(146, 191, 233);
		position: relative;
		border: 1px solid #e91f15;
	}
	.ent-search-bar{
		padding-top: 10px;
		padding-bottom: 10px;
	}
	.ent-serach-wrap input{
		height: 100%;
		width: 100%;
		padding: 0px;
		display: block;
		border: 0px;
		position: absolute;
		left: 0px;
	}
	.ent-serach-wrap button{
		cursor: pointer;
		display: block;
		width: 40px;
		height: 100%;
		position: absolute;
		right: 0;
		top:0;
		text-indent: -9999px;
		overflow: hidden;
		border: 0;
		border-left: 1px solid #e91f15;
		background-color: red;
		background-image: url(http://s2.apptuan.bdimg.com/static/mobile/list/home/img/icon-search_e826fa96.png);
		background-position: center;
		background-repeat: no-repeat;
		background-size: 17px 17px;
	}
	.ent-category ul li{
		float: left;
		width: 33%;
		list-style: none;
		background-color: rgb(214, 240, 250);
	}
	.ent-category-list a{
		text-decoration: none;
		display: block;
		margin: 10px auto 10px;
		width: 50px;
		font-size: 14px;
		color:#4c4c4c;
		text-align: center;
		line-height: 40px;
		background-color: red;
		background-repeat: no-repeat;
		background-position: center;
		background-size: 17px 17px;
		text-indent: -1000px;
		border-radius: 10px;
	}
	.ent-category ul:last-child:after{
		content:" ";
		clear: both;
		display: block;
		height: 1px;
	}
	.c-ent-category-film a{
		background-image: url(http://s2.apptuan.bdimg.com/static/mobile/list/home/img/dianying_04576cc0.png);
	}
	.c-ent-category-hotel a{
		background-image: url(http://s2.apptuan.bdimg.com/static/mobile/list/home/img/jiudian_66d9a71d.png);

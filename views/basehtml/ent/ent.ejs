	}
	.c-ent-category-woman a{
		background-image: url(http://s2.apptuan.bdimg.com/static/mobile/list/home/img/liren_a93424df.png);
	}
	.ent-good-recommend h2{
		margin-top: 8px;
		margin-bottom: -4px;
		margin-left: 8px;
		color: #333;
		font-weight: normal;
		font-size: 14px;
	}
	.w-goods{
		border-bottom: 1px solid rgb(10, 122, 175);
	}
	.w-goods ul li{
		padding-top: 5px;
	}
	.w-goods a{
		display: block;
	}
	.w-goods a,.w-goods ins,.w-goods ul{
		text-decoration: none;
		list-style: none;
	}
	.w-goods .other span{
		float: right;
		margin-right: 20px;
	}
	.w-goods a:after{
		content: " ";
		display: block;
		height: 0px;
		overflow: hidden;
		visibility: hidden;
		clear: both;
	}
	.img-wrapper{
		float: left;
	}
	.img-wrapper img{
		height: 80px;
	}
</style>

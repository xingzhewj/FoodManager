<style type="text/css">
	.nav-list{
		padding: 8px 15px;
		margin-bottom: 20px;
		list-style: none;
		background-color: #8DC8F0;
		border-radius: 4px;
		margin: 0px;
	}
	.nav-list>li{
		display: inline-block;
	}
	.nav-list li:nth-child(2):before{
		content: ">";
	}
	.nav-list li:nth-child(3):before{
		content: ">";
	}
	.nav-list>li a{
		text-decoration: none;
		color:#555;
	}
</style>
<div class="nav-contain">
	<ol class="nav-list">
		<% navlist.forEach(function(navitem){ %>
			<li><a href="<%= navitem.url %>"><%= navitem.title %></a></li>
		<% }) %>
	</ol>
</div>